  "/generic/{table}": {
   "delete": {
    "description": "Delete Datas\n\u003cbr\u003e",
    "operationId": "GenericController.Delete",
    "parameters": [
     {
      "description": "Value of the table",
      "in": "path",
      "name": "table",
      "required": true,
      "type": "string"
     },
     {
      "description": "needed on a rows level request (value=all for post/put method or a get/delete all)",
      "in": "query",
      "name": "rows",
      "type": "string"
     },
     {
      "description": "needed on a columns level request (POST/PUT/DELETE with no rows query params) will set up a view on row level (show only expected columns)",
      "in": "query",
      "name": "columns",
      "type": "string"
     },
     {
      "description": "sets up a sql order in query (don't even try to inject you little joker)",
      "in": "query",
      "name": "orderby",
      "type": "string"
     },
     {
      "description": "sets up a sql direction in query (ex.ASC|DESC) (don't even try to inject you little joker)",
      "in": "query",
      "name": "dir",
      "type": "string"
     },
     {
      "description": "(Optionnal) Value of limit",
      "in": "query",
      "name": "limit",
      "type": "string"
     },
     {
      "description": "(Optionnal) Value of offset",
      "in": "query",
      "name": "offset",
      "type": "string"
     },
     {
      "description": "activate a lightest response (name only)",
      "in": "query",
      "name": "shallow",
      "type": "string"
     },
     {
      "description": "set 'enable' to activate a response without the main response format",
      "in": "query",
      "name": "rawview",
      "type": "string"
     },
     {
      "description": "(Optionnal) Value of export",
      "in": "query",
      "name": "export",
      "type": "string"
     },
     {
      "description": "(Optionnal) Value of filename",
      "in": "query",
      "name": "filename",
      "type": "string"
     },
     {
      "description": "(Optionnal) Value of filter_status",
      "in": "query",
      "name": "filter_status",
      "type": "string"
     },
     {
      "description": "set a filter command line compose as (key~value(+|))",
      "in": "query",
      "name": "filter_line",
      "type": "string"
     },
     {
      "description": "(Optionnal) Value of command_row",
      "in": "query",
      "name": "command_row",
      "type": "string"
     },
     {
      "description": "(Optionnal) Value of id",
      "in": "query",
      "name": "id",
      "type": "string"
     },
     {
      "description": "(Optionnal) Value of group_by",
      "in": "query",
